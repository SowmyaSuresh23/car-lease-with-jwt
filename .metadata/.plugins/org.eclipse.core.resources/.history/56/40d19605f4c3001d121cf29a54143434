package com.sowmya.carleaseplatform.dto;

import lombok.AllArgsConstructor;
import lombok.Builder;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@Builder
@NoArgsConstructor
@AllArgsConstructor
public class LeaseDetailsResponse {

  private Long leaseId;
  private Float duration;
  private Float interestRate;
  private Double leaseRate;
  private String customerEmail;
  private String carNumber;
  private CarDetailsResponse carDetails = new CarDetailsResponse();
  private CustomerDetailsResponse customerDetails = new CustomerDetailsResponse();



}
